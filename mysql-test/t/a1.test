--source include/have_innodb.inc

CREATE COMPRESSION_DICTIONARY foo ('abbccc');
CREATE COMPRESSION_DICTIONARY bar ('abbccc');
CREATE TABLE t1(
  id INT,
  a BLOB COLUMN_FORMAT COMPRESSED WITH COMPRESSION_DICTIONARY bar,
  b INT,
  c BLOB COLUMN_FORMAT COMPRESSED WITH COMPRESSION_DICTIONARY foo,
  d INT
) ENGINE=InnoDB;
CREATE TABLE t2(
  id INT,
  a BLOB COLUMN_FORMAT COMPRESSED WITH COMPRESSION_DICTIONARY bar,
  b INT,
  c BLOB COLUMN_FORMAT COMPRESSED WITH COMPRESSION_DICTIONARY bar,
  d INT
);
CREATE TABLE t3(
  id INT
);

SELECT * FROM information_schema.xtradb_zip_dict;
SELECT * FROM information_schema.xtradb_zip_dict_cols;

DROP TABLE t1, t2, t3;
